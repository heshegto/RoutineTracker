<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="TimeButton">
        <attr name="name" format="string" default="default"/>
        <attr name="default_hours" format="integer" default="0"/>
        <attr name="default_minute" format="integer" default="0"/>
        <attr name="default_seconds" format="integer" default="0"/>
        <attr name="colorInactive" format="color" default="@color/grey"/>
        <attr name="colorActive" format="color" default="@color/grey"/>
    </declare-styleable>
</resources>